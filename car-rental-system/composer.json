{
    "name": "car-rental-system/car-rental-system",
    "type": "project",
    "description": "Sistem penyewaan mobil menggunakan CodeIgniter 4",
    "keywords": [
        "codeigniter",
        "car-rental",
        "php",
        "framework"
    ],
    "homepage": "https://carrental.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Car Rental System Team",
            "email": "team@carrental.com",
            "homepage": "https://carrental.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "codeigniter4/framework": "^4.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    },
    "support": {
        "source": "https://github.com/carrental/car-rental-system",
        "issues": "https://github.com/carrental/car-rental-system/issues",
        "forum": "https://forum.codeigniter.com/"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}